{
  "AllowedHosts": "*",
  "apollo": {
    "AppId": "apollo-client",
    "Cluster": "dev",
    "LocalCacheDir": "./",
    "Secret": "ffd9d01130ee4329875ac3441c0bedda",
    "Namespaces": [ "application", "IdentityServer.json", "application.json" ],
    "Meta": {
      "DEV": "http://security-config-apolloconfigservice:8080/",
      "PRO": "http://security-config-apolloconfigservice:8080/"
    }
  },
  "PasswordLength": 50000,
  "TokenHandleLength": 200,
  "CerName": "idsrv4.pfx",
  "CerPwd": "Hexin@2021",
  "RsaPrivateKey": "",
  "RsaPublicKey": "",
  "AuthDataGrpcAddress": "http://localhost:5034",
  "UserDataGrpcAddress": "http://localhost:5134",
  "IdentityServerUrls": [ "http://*:5100" ],
  "CacheExpireSeconds": 60,
  "UseHxRedis": true,
  "OpenTraceInLogMessageHttpHandler": true,
  "IdentityServerRedisOptions": {
    "Password": "123456",
    "AllowAdmin": true,
    "Ssl": false,
    "ConnectTimeout": 6000,
    "ConnectRetry": 2,
    "Database": 0,
    "Hosts": [
      {
        "Host": "127.0.0.1",
        "Port": 6379
      }
    ]
  },
  "RiskSwitch": false,
  "RiskHttpTimeOutMilliseconds": 10,
  "RiskCheckUrl": "http://localhost:5001",

  "ExcludeLogFilter": {
    "Switch": true,
    "SourceContext": [ "System", "Microsoft.AspNetCore.Routing.EndpointMiddleware", "Microsoft" ],
    "RequestPath": ["/health", "/"]
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Async", "Serilog.Sinks.Console", "Serilog.Enrichers.CorrelationId", "Serilog.Enrichers.ClientInfo" ],
    "MinimumLevel": {
      "ControlledBy": "$mySwitch",
      "Override": {
        "Microsoft": "Debug"
      }
    },
    "LevelSwitches": {
      "$mySwitch": "Information",
      "$consoleSwitch": "Information"

    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "{Timestamp:HH:mm:ss.fff zzz} [{Level}] {SourceContext} {CorrelationId} {ClientIp} {Message}  {NewLine}{Exception}",
                "levelSwitch": "$consoleSwitch"
              }

            },
            {
              "Name": "File",
              "Args": {
                "path": "./logs/log.json",
                "rollingInterval": "Day",
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
                "levelSwitch": "$mySwitch"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "WithCorrelationIdHeader", "WithClientIp", "FromLogContext" ],
    "Properties": {
      "Application": "Normandy.Identity.Server"
    }
  }
}
